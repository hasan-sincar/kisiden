<IfModule mod_rewrite.c>
    RewriteEngine On
    RewriteBase /
    
    RewriteRule ^.well-known/acme-challenge/(.*) /.well-known/acme-challenge/$1 [L]

    RewriteRule ^_next/(.*) /.next/$1 [L]
     # Allow access to static files with specific extensions
    RewriteCond %{REQUEST_URI} \.(js|txt|css|svg|jpg|jpeg|png|gif|ico)$
    RewriteRule ^ - [L]
    
    RewriteRule ^index.html http://127.0.0.1:8003/$1 [P]
    RewriteRule ^index.php http://127.0.0.1:8003/$1 [P]
    RewriteRule ^/?(.*)$ http://127.0.0.1:8003/$1 [P]
</IfModule>

